{
  "manifest_version": 3,
  "name": "LinkedInAI",
  "version": "1.0",
  "description": "LinkedIn",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/*",
    "https://127.0.0.1:8000/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/feed/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}